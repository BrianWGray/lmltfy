@using lmltfy.Models
@model lmltfy.Models.SearchResultsViewModel

@Html.DisplayFor(a=>a.BrandingModel)
@Html.Partial("SearchBarParitalView", new SearchModel())

@Html.HiddenFor(a=>a.SearchModel.Search, new {@Id = "SearchQuery"})
@Html.HiddenFor(a=>a.RedirectModel.RedirectUrl, new { @Id = "RedirUrl" })
@Html.HiddenFor(a => a.RedirectModel.Key, new { @Id = "Key" })


@section Scripts
{
    @Scripts.Render("~/bundles/search")

}
